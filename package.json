{
	"name": "alembic-nft-minting-contracts",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"clean": "rimraf ./artifacts/ ./flatten/ ./coverage/ ./coverage.json ./cache/ ./dist",
		"compile": "hardhat compile",
		"postcompile": "copyfiles -u 2 \"artifacts/typechain/**/*.d.ts\" dist",
		"test": "npx hardhat test",
		"coverage": "npx hardhat coverage",
		"lint": "prettier --check \"contracts/**/*.{sol,ts,js}\"",
		"prettier": "prettier --write \"contracts/**/*.{sol,ts,js}\" \"test/**.ts\"",
		"gas": "cross-env REPORT_GAS=true hardhat test",
		"deploy:devnet": "npx hardhat --network matic rental:deploy --fees-collector 0xf845b2501A69eF480aC577b99e96796c2B6AE88E"
	},
	"dependencies": {
		"@openzeppelin/contracts": "^4.7.3"
	},
	"devDependencies": {
		"@ethersproject/abi": "^5.7.0",
		"@ethersproject/providers": "^5.7.2",
		"@nomicfoundation/hardhat-chai-matchers": "^1.0.4",
		"@nomicfoundation/hardhat-network-helpers": "^1.0.6",
		"@nomicfoundation/hardhat-toolbox": "^2.0.0",
		"@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
		"@nomiclabs/hardhat-etherscan": "^3.1.1",
		"@primitivefi/hardhat-dodoc": "^0.2.3",
		"@typechain/ethers-v5": "^10.1.0",
		"@typechain/hardhat": "^6.1.3",
		"@types/chai": "^4.3.3",
		"@types/mocha": "^10.0.0",
		"@types/node": "^18.11.3",
		"chai": "^4.3.6",
		"copyfiles": "^2.4.1",
		"cross-env": "^7.0.3",
		"dotenv": "^10.0.0",
		"eslint": "^7.29.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-import-resolver-typescript": "^3.5.2",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-promise": "^5.1.0",
		"ethers": "^5.7.2",
		"ethers-merkletree": "^1.0.16",
		"hardhat": "^2.12.0",
		"hardhat-contract-sizer": "^2.6.1",
		"hardhat-deploy": "^0.11.19",
		"hardhat-gas-reporter": "^1.0.9",
		"prettier": "^2.3.2",
		"prettier-plugin-solidity": "^1.0.0-beta.24",
		"rimraf": "^3.0.2",
		"solhint": "^3.3.7",
		"solidity-coverage": "^0.8.2",
		"ts-generator": "^0.1.1",
		"ts-node": "^10.9.1",
		"typechain": "^8.1.0",
		"typescript": "^4.8.4"
	}
